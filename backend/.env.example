# Server
PORT=5000
NODE_ENV=production

# Frontend URL (for CORS)
FRONTEND_URL=https://titleiq.titleslice.com

# JWT Secret (generate with: openssl rand -base64 32)
JWT_SECRET=your-secret-jwt-key-change-this

# Encryption Secret for API keys (generate with: openssl rand -base64 32)
ENCRYPTION_SECRET=your-secret-encryption-key-change-this

# Groq API Key (free tier)
# Get yours at: https://console.groq.com/keys
GROQ_API_KEY=your-groq-api-key
